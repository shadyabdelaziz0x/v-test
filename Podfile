# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def sharedPods
  use_frameworks!
    pod 'R.swift'
    pod 'Alamofire'
    pod "PromiseKit"
    pod 'ReachabilitySwift'
    pod 'SQLite.swift'
    pod "SQLiteMigrationManager.swift"
    pod 'AwaitKit', :git => 'https://github.com/yannickl/AwaitKit.git', :branch => 'master'
    pod 'SDWebImage'
end

target 'vmovies' do
  sharedPods
end
